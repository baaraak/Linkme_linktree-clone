{
  "root": true,
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "parser": "babel-eslint",
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": [],
  "rules": {
    "react/prop-types": 0,
    "react/no-unescaped-entities": 0
  }
}
